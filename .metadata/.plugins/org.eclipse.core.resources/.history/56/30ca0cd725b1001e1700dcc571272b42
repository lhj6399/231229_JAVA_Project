package day08;

public class Method05 {

	public static void main(String[] args) {
		/*
		 * 
		 */

		boolean b = a(6);
		
	}

	/* 정수 하나가 주어지면 이 정수가 소수인지 아닌지 판별(true/false)
	 * 
	 */
	
	public static boolean a (int num) {
		int count = 0;
		
			for (int j=1; j<=num; j++) {
				if (num % j == 0) {
					count++;
				}
			}
			if(count == 2) {
				return true;
			} else if (count != 2) {
				return false;
			}
		}
		
	}
	
	
}
