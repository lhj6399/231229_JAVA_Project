package day05;

public class Array03 {

	public static void main(String[] args) {
		
		int arr[] = new int[] {10,56,78,89,48,62,93,50};
		/* arr배열의 합계와 평균을 출력
		 * 최대값 / 최소값 출력
		 */
		
		int sum = 0;
		int avg = 0;
		
		for(int i=0; i<arr.length; i++) {
			System.out.print(arr[i]+" ");
			sum = sum+arr[i];
		}
		avg = sum / arr.length;
		System.out.println();
		System.out.println(sum);
		System.out.println(avg);
	}

}
