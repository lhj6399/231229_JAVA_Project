package day06;

import java.util.Scanner;

public class ArrayEx01 {

	public static void main(String[] args) {
		/* 10개의 배열을 담을 수 있는 arr 생성
		 * 1~10까지 값을 입력한 후 출력
		 */
		int arr [] = new int [10];
		int num = 0;
		
		Scanner scan = new Scanner(System.in);
		num = scan.nextInt();
		for(int i=0;i<arr.length;i++) {
			num = arr[i];
			System.out.println(num);
		}
		
		
		scan.close();
		
		
		
	}

}
