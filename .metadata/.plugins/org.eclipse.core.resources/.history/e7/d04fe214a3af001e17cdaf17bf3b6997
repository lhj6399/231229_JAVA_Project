package day06;

public class ArrayEx02 {

	public static void main(String[] args) {
		/* 1~50 까지의 랜덤수를 10개 생성하여 배열에 저장
		 * 오름차순 정렬 후 출력
		 */
		int arr[] = new int[10];
		
		for(int i=0; i<arr.length; i++) {
			arr[i] = (int)(Math.random()*50); //1부터 50까지의 랜덤 수 생성
			System.out.print(arr[i]+" ");
		}

	}

}
