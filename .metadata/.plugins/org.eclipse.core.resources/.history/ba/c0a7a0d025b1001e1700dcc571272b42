package day08;

public class Method05 {

	public static void main(String[] args) {
		/*
		 * 
		 */

		int a = a(6);
		
	}

	/* 정수 하나가 주어지면 이 정수가 소수인지 아닌지 판별(true/false)
	 * 
	 */
	
	public static void a (int num) {
		int count = 0;
		for (int i=2; i<=num; i++) {
			count = 0;
			for (int j=1; j<=i; j++) {
				if (i % j == 0) {
					count++;
				}
			}
			if(count == 2) {
				count = i;
			}
		}
		
	}
	
	
}
