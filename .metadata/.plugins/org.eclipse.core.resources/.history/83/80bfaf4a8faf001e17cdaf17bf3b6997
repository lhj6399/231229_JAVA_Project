package day06;

public class Array05 {

	public static void main(String[] args) {
		/* 문자열을 담는 배열을 생성
		 * 
		 */
		String arr[] = new String[5];
		arr[0] = "홍길동";
		arr[1] = "김영이";
		arr[2] = "박순이";
		arr[3] = "최영철";
		arr[4] = "박순철";
		for(int i=0; i<arr.length; i++) {
			System.out.println(arr[i]+" ");
		}
		
		
		
	}

}
