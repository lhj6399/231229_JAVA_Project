package day06;

public class ArrayEx02 {

	public static void main(String[] args) {
		/* 1~50 까지의 랜덤수를 10개 생성하여 배열에 저장
		 * 오름차순 정렬 후 출력
		 */
		int arr [] = new int [10];
		
		for(int i=0;i<arr.length;i++) {
			arr[i] = i+1;
			System.out.print(arr[i]+" ");
		}
		
		System.out.println();
		
		int min = 0;
		int max = arr.length;
		for(int i=0; i<arr.length;i++) {
		int random = (int)(Math.random()*max)+min; 
		int temp = arr[i];
		arr[i] = arr[random];
		arr[random] = temp;
		
		}
		
		

	}

}
