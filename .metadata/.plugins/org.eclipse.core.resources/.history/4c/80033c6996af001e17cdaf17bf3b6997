package day06;

import java.util.Scanner;

public class ArrayEx01 {

	public static void main(String[] args) {
		/* 10개의 배열을 담을 수 있는 arr 생성
		 * 1~10까지 값을 입력한 후 출력
		 */
		int arr [] = new int [10];
		
		for(int i=0;i<arr.length;i++) {
			arr[i] = i+1;
			System.out.println(arr[i]+" ");
		}
		
		
		scan.close();
		
		
		
	}

}
